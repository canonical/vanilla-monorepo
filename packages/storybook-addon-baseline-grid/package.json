{
  "name": "@canonical/storybook-addon-baseline-grid",
  "version": "0.4.1-experimental.0",
  "type": "module",
  "files": ["dist"],
  "types": "dist/types/index.d.ts",
  "author": "Bartek Szopka <bartek.szopka@canonical.com>",
  "description": "Displays the baseline grid",
  "keywords": ["storybook-addons", "storybook-addons"],
  "repository": {
    "type": "git",
    "url": "https://github.com/canonical/ds25"
  },
  "bugs": {
    "url": "https://github.com/canonical/ds25/issues"
  },
  "license": "LGPL-3.0",
  "exports": {
    ".": "./dist/esm/index.js",
    "./preview": "./dist/esm/preview.js",
    "./preset": "./dist/esm/preset.js",
    "./manager": "./dist/esm/manager.js",
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "bun run build:package",
    "build:package": "bun run build:package:tsc",
    "build:package:tsc": "tsc -p tsconfig.build.json",
    "storybook": "storybook dev -p 6007 --no-open --host 0.0.0.0",
    "check": "bun run check:biome && bun run check:ts",
    "check:fix": "bun run check:biome:fix && bun run check:ts",
    "check:biome": "biome check src *.json",
    "check:biome:fix": "biome check --write src *.json",
    "check:ts": "tsc --noEmit"
  },
  "dependencies": {
    "@storybook/icons": "^1.2.10"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@canonical/biome-config": "^0.4.0-experimental.0",
    "@canonical/typescript-config-react": "^0.4.0-experimental.0",
    "@storybook/addon-essentials": "^8.4.7",
    "@storybook/addon-interactions": "^8.4.7",
    "@storybook/addon-links": "^8.4.7",
    "@storybook/blocks": "^8.4.7",
    "@storybook/react": "^8.4.7",
    "@storybook/react-vite": "^8.4.7",
    "@storybook/test": "^8.4.7",
    "@types/node": "^22.10.1",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.2",
    "@vitejs/plugin-react": "^4.3.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "storybook": "^8.4.7",
    "typescript": "^5.7.2",
    "vite": "^6.0.3"
  },
  "peerDependencies": {
    "storybook": "^8.4.7"
  },
  "storybook": {
    "displayName": "DS Baseline Grid",
    "supportedFrameworks": [
      "react",
      "vue",
      "angular",
      "web-components",
      "ember",
      "html",
      "svelte",
      "preact",
      "react-native"
    ],
    "icon": "https://user-images.githubusercontent.com/321738/63501763-88dbf600-c4cc-11e9-96cd-94adadc2fd72.png"
  }
}
